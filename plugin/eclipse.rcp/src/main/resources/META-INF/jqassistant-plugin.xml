<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0">
    <description>Provides a scanner for Eclipse RCP modules.</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.eclipse.rcp.PluginXmlFileDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.eclipse.rcp.XmlElement</class>
        <class>com.buschmais.jqassistant.plugin.eclipse.rcp.XmlAttribute</class>
    </model>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.eclipse.rcp.EclipseRcpProjectScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>